{
  "sourceMaps": true,
  "presets": [
    "es2017"
  ],
  "plugins": [
    [
      "syntax-async-functions"
    ],
    [
      "transform-es2015-modules-commonjs"
    ],
    [
      "transform-object-rest-spread"
    ],
    [
      "transform-react-jsx",
      {
        "pragma": "h"
      }
    ]
  ]
}
